// Constants syntax
const y[255]: int = 100;
const char: str = 'a';
const text: str = "Hello World";

// Variables syntax
var p[10]: int;
var o: int;
var v_text: str;
var v_char: str;

// Operations syntax
var result: int = 0;

mul p, 3; // Store the result in the variable itself
mul (x, 3) -> result; // Store the result in an other variable called result

add p, 1; // Store the result in the variable itself
add (x, 1) -> result; // Store the result in an other variable called result

div p, 2; // Store the result in the variable itself
div (x, 2) -> result; // Store the result in an other variable called result

sub p, 10; // Store the result in the variable itself
sub (x, 10) -> result; // Store the result in an other variable called result

